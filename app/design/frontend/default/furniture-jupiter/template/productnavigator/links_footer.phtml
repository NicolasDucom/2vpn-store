<?php $array = $this->getShow();?>
<ul class="product-navigator-links">
	<?php $i = 0; foreach ($array as $key=>$show):?>
		<li class="item<?php if($i == 0): ?> first<?php endif;?><?php if($i == (count($array)-1)):?> last<?php endif;?>">
			<a href="<?php echo $show['url']?>">
				<span><?php echo $show['title']?></span>
			</a>
		</li>
		<?php $i++; ?>
	<?php endforeach;?>
</ul>